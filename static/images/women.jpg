<script>
            async function loadTemplateFrames(url) {
                const template_elems = $('.canva-template-ad-item');
                template_elems.css({opacity: 0.2});
                $('.loading').show();

                const response = await fetch(url, {
                    method: 'POST',
                    headers: {
                        'X-CSRFToken': getCookie('csrftoken', ''),
                        'Accept': 'application/json'
                    },
                    body: new URLSearchParams({
                        canva_id: 'MADQ4_olxTQ',
                        width: '4818',
                        height: '5346',
                        keywords: 'women, yoga, silhouettes, water, reflection, mirroring, water reflection, pose, posing, yoga pose, meditation, stretching, wellness, well\u002Dbeing, exercise, adults, asia, girls, morning, outdoors, pair, people, stance, sunrise, sunset',
                    }),
                });
                const new_templates = await response.json();

                let frames_loaded = 0;
                const onFrameLoaded = () => {
                    frames_loaded++;
                    if (frames_loaded == 5) {
                        template_elems.css({opacity: 1});
                        $('.loading').hide();
                    }
                };

                for(let i = 0; i < 5; i++) {
                    const new_template = new_templates[i];
                    const template_elem = template_elems.eq(i);
                    // replace link href
                    template_elem.find('.thumbnail-container').attr('href', new_template.designUrl);
                    // replace mount parameters
                    template_elem.find('.template-mount').css({
                        top: `${new_template.mountTop/new_template.thumbnailHeight * 100}%`,
                        left: `${new_template.mountLeft/new_template.thumbnailWidth * 100}%`,
                        height: `${new_template.mountHeight/new_template.thumbnailHeight * 100}%`,
                        width: `${new_template.mountWidth/new_template.thumbnailWidth * 100}%`
                    });
                    // replace template image
                    template_elem.find('.template-frame').off('load');
                    template_elem.find('.template-frame').attr('src', new_template.thumbnailUrl);
                    template_elem.find('.template-frame').on('load', onFrameLoaded);
                    // replace doctype
                    template_elem.find('.template-doctype').text(new_template.doctypeName);
                }
            }

            $('.template-type').click(e => {
                e.preventDefault();
                e.stopPropagation();

                const elem = $(e.currentTarget);
                if (elem.hasClass('selected'))
                    return;

                $('.template-type').removeClass('selected');
                elem.addClass('selected');

                const url = elem.data('category-url');
                loadTemplateFrames(url);
            });

            $(document).ready(function() {
                const url = "/images/canva-template-frames/top-5/";
                loadTemplateFrames(url);
            });
        </script>
